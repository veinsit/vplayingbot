{
    "extends": "tslint:recommended",
    "rulesDirectory": [],
    "rules": {
        "max-line-length": {
            "options": [120]
        },
        "new-parens": true,
        "no-arg": true,
        "no-bitwise": true,
        "no-conditional-assignment": true,
        "no-consecutive-blank-lines": false,
        "no-console": {
            "options": [
                "debug",
                "info",
                "log",
                "time",
                "timeEnd",
                "trace"
            ]
        },
        "quotemark" : false,
        "variable-name" : false,
        "semicolon" : false,
        "curly" : false,
        "trailing-comma" : false,
        "only-arrow-functions" : false,
        "no-var-requires" : false,
        "object-literal-sort-keys" : false
    },
    "jsRules": {
        "max-line-length": {
            "options": [120]
        }
    }
}